{
    "name": "Stuct Json",
    "version": "3.1.2.4567",
    "manifest_version":2,
    "permissions":[
        "activeTab",
        "http://localhost:4200/"
    ],
    "browser_action" :{
        "default_popup":"popup.html",
        "default_icon": "/logo.png"

    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"],
            "css":["style.css"]
          }
      ],
    "icons" :{
            "128":"logo.png"
    }
   
}